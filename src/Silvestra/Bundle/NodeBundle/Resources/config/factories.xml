<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="silvestra_node.factory.route" class="Silvestra\Bundle\NodeBundle\Factory\RouteFactory">
            <argument type="service" id="silvestra_node.routing.generator" />
            <argument type="service" id="tadcka_routing.manager.route" />
            <argument type="service" id="silvestra_node.routing.helper" />
        </service>

        <service id="silvestra_node.factory.seo" class="Silvestra\Bundle\NodeBundle\Factory\SeoFactory">
            <argument type="service" id="silvestra_seo.manager.seo_metadata" />
        </service>

    </services>
</container>
