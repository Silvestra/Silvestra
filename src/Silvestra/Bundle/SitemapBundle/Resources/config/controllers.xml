<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <!--Silvestra sitemap controller-->
        <service id="silvestra_sitemap.controller.sitemap" class="Silvestra\Bundle\SitemapBundle\Controller\SitemapController">
            <argument type="service" id="silvestra_sitemap.dumper.sitemap"/>
            <argument>%silvestra_sitemap.http_cache%</argument>
        </service>


    </services>

</container>
