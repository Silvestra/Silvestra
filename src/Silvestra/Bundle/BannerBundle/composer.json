{
    "name":         "silvestra/banner-bundle",
    "homepage":     "https://github.com/Silvestra/BannerBundle",
    "description":  "Silvestra banner bundle",
    "keywords":     ["Banner"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors": [
        {
            "name":   "Tadas Gliaubicas",
            "email":  "tadcka89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle" : ">=2.3",
        "silvestra/banner": "0.1.*@dev",
        "silvestra/locale-bundle": "0.1.*@dev",
        "silvestra/media-bundle": "0.1.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "suggest": {
        "components/bootstrap": "Bootstrap library.",
        "components/jquery": "JQuery library.",
        "silvestra/bootstrap-sweetalert": "Bootstrap sweet alert library"
    },
    "autoload": {
        "psr-4": {
            "Silvestra\\Bundle\\BannerBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
