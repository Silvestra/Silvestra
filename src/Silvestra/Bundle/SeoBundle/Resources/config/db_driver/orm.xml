<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="silvestra_seo.manager.seo_metadata.class">Silvestra\Bundle\SeoBundle\Doctrine\EntityManager\SeoMetadataManager</parameter>
    </parameters>

    <services>

        <!--Default SeoMetadata manager-->
        <service id="silvestra_seo.manager.seo_metadata.default" class="%silvestra_seo.manager.seo_metadata.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument>%silvestra_seo.model.seo_metadata.class%</argument>
        </service>

    </services>
</container>
